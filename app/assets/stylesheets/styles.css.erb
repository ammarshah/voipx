.blue-bubble {
    border-color: #4691e1;
    background: #5c9ee5;
    clear: both;
    float: left;
    margin-bottom: 20px;
    padding: 8px 30px;
    position: relative;
    max-width: 100%;
    word-wrap: break-word;
    border-radius: 8px;
    color: #f1f2f2;
    margin-right: 10px;
    width: 65%;
    overflow: visible;
}


.blue-bubble:before, .blue-bubble:after {
    border-radius: 20px / 10px;
    content: '';
    display: block;
    position: absolute;
}

.blue-bubble:before {
    border: 10px solid transparent;
        border-bottom-color: transparent;
    border-bottom-color: #5c9ee5;
    left: -10px;
    bottom: 0;
    z-index: -2;
}
.blue-bubble:after {
    border: 8px solid transparent;
        border-bottom-color: transparent;
    border-bottom-color: #5c9ee5;
    left: -8px;
    bottom: 1px;
}

